{
  "name": "WikiFocus",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Chrome extension to focus the search bar when you open Wikipedia, so that you can type on it without having to click",
  "homepage_url": "https://github.com/TrinTragula/WikiFocus",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "permissions": [
    "https://*.wikipedia.org/*",
    "http://*.wikipedia.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.wikipedia.org/*",
        "http://*.wikipedia.org/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}